module c where

extern printf(format: str, ...): i32;
extern malloc(bytes: usize): void*;
extern free(ptr: void*): void;
extern memset(s: void*, c: i32, n: usize): void*;

proc create_array(szty: usize, amt: usize): i32* {
    let n: usize = szty*amt;
    let arr: i32* = i32* c::malloc(n);

    for let i: i32 = 0; i < i32 amt; i = i+1; {
        arr[i] = 9;
    }

    return arr;
}
