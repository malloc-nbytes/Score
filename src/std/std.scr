export proc assert_i32_eq(l: i32, r: i32): void {
    if l != r {
        printf("assert_i32_eq failure. left = %d, right = %d\n", l, r);
        exit(1);
    }
}

export proc assert_usize_eq(l: usize, r: usize): void {
    if l != r {
        printf("assert_usize_eq failure. left = %d, right = %d\n", l, r);
        exit(1);
    }
}

