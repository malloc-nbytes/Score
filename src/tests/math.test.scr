proc ret0(void): i32 {
     printf("ret0\n");
     return 0;
}

proc addition_in_ret(void): i32 {
     printf("addition_in_ret\n");
     return 1+2;
}

proc multiplication_in_ret(n: i32): i32 {
     printf("multiplication_in_ret\n");
     return n*3*2;
}

proc div_in_ret(n: i32): i32 {
     printf("div_in_ret\n");
     return n/2;
}

proc main(void): i32 {
     if ret0() != 0 {
         exit(1);
     }
     if addition_in_ret() != 3 {
         exit(1);
     }
     if multiplication_in_ret(3) != 18 {
         exit(1);
     }
     if div_in_ret(10) != 5 {
         exit(1);
     }

     return 0;
}
