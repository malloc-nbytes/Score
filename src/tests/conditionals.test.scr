proc max(a: i32, b: i32): i32 {
    printf("max\n");
    if a > b {
        return a;
    }
    return b;
}

proc max_usize(a: usize, b: usize): usize {
    printf("max_usize\n");
    if a > b {
        return a;
    }
    return b;
}

proc if_else_if(a: i32, b: i32): i32 {
    printf("if_else_if a: %d b: %d\n", a, b);
    if a > b {
        return 1;
    }
    else if b > a {
        return 2;
    }
    return 3;
}

proc main(void): i32 {
    if max(2, 1) != 2 {
        printf("max failed\n");
        exit(1);
    }

    if max(1, 2) != 2 {
        printf("max failed\n");
        exit(1);
    }

    if max_usize(1, 2) != 2 {
        printf("max_usize failed\n");
        exit(1);
    }

    if max_usize(2, 1) != 2 {
        printf("max_usize failed\n");
        exit(1);
    }

    if if_else_if(1, 2) != 2 {
        printf("if_else_if failed\n");
        exit(1);
    }

    if if_else_if(2, 1) != 1 {
        printf("if_else_if failed\n");
        exit(1);
    }

    if if_else_if(1, 1) != 3 {
        printf("if_else_if failed\n");
        exit(1);
    }

    return 0;
}
