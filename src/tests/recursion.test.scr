proc rectest1(depth: i32): i32 {
     if depth == 10 {
         return depth;
     }
     return rectest1(depth+1);
}

proc rectest2(depth: i32): i32 {
     if depth == 1000 {
         return depth;
     }
     return rectest2(depth+1);
}

proc main(void): i32 {
     printf("rectest1\n");
     if rectest1(0) != 10 {
         exit(1);
     }

     printf("rectest2\n");
     if rectest2(0) != 1000 {
         exit(1);
     }

     return 0;
}
