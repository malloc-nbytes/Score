export proc ret_passed(a: i32): i32 {
    -- printf("ret_passed\n");
    return a;
}

export proc add_5(a: i32): i32 {
    -- printf("add_5\n");
    return a+5;
}

export proc run_pass_by_copy(void): i32 {
    if ret_passed(101) != 101 {
        printf("ret_passed failed\n");
        return 1;
    }

    if add_5(100) != 105 {
        printf("add_5 failed\n");
        return 1;
    }

    return 0;
}
